{
  "name": "@shingo/sf-api-client",
  "version": "1.0.0-alpha.1",
  "description": "Client library for shingo sf api",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shingoinstitute/shingo-sf-api.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s clean && run-p build:*",
    "build:proto": "mkdir -p build/proto && cp -R ../../proto build/",
    "build:main": "tsc -b .",
    "clean": "trash build test docs",
    "prepack": "npm run build && cp -Rav build/* . | ../../scripts/parseEmitted.sh build.emit",
    "postpack": "../../scripts/postpack.sh"
  },
  "author": "Abraham White <abelincoln.white@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^10.9.4",
    "npm-run-all": "^4.1.3",
    "trash-cli": "^1.4.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.3.0",
    "@shingo/sf-api-shared": "^1.0.0-alpha.1",
    "@types/jsforce": "^1.8.16",
    "grpc": "^1.15.0"
  }
}
